@{
    ViewBag.Title = "Slice of Pie";
    ViewBag.Message = "Viewer";
}
@section home {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}
<section class="viewer">
     <div class="content-wrapper">
         @Html.Raw(ViewBag.Document)
     </div>
</section>